{
  "name": "@shaggytools/template-typescript-npm",
  "libraryName": "library-name",
  "version": "1.0.0",
  "description": "A starter template for publishing a package with Node, Typescript, Rollup, Jest, and JSDoc.",
  "engines": {
    "node": ">=12.14.0 <13.0.0",
    "npm": "6.11.3"
  },
  "main": "dist/bundle.min.js",
  "module": "dist/module/index.js",
  "browser": "dist/browser/iife.js",
  "unpkg": "dist/bundle.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/*",
    "dist/cjs/*",
    "dist/module/*",
    "dist/browser/*",
    "dist/types/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "yarn clean:dist",
    "build": "rollup -c rollup.config.ts",
    "prebuild:dev": "yarn cleanDev:dist && yarn copy:packageJsonToDev",
    "build:dev": "cross-env ROLLUP_WATCH=true rollup -c rollup.config.ts",
    "prebuild:docs": "yarn docs:clean",
    "build:docs": "jsdoc -c ./jsdoc.js",
    "prebuild:docsDev": "yarn cleanDev:docs",
    "build:docsDev": "cross-env JSDOC_DEV=true jsdoc -c ./jsdoc.js",
    "build:ts": "tsc --module commonjs",
    "clean": "concurrently \"yarn:clean:*\" \"yarn:cleanDev:*\"",
    "clean:dist": "rimraf dist/*",
    "cleanDev": "rimraf dev/*",
    "cleanDev:dist": "rimraf dev/dist/*",
    "cleanDev:packageJson": "rimraf dev/package.json",
    "cleanDev:docs": "rimraf dev/docs/*",
    "cleanDev:tests": "yarn test:clearCache && rimraf dev/coverage/*",
    "cleanAll": "yarn cleanDev:packageJson && rimraf dist && rimraf dev",
    "cleanALL:Everything": "yarn  cleanAll:Dev && rimraf node_modules",
    "precopy:packageJsonToDev": "yarn cleanDev:packageJson",
    "copy:packageJsonToDev": "copyfiles ./package.json ./dev",
    "docs:clean": "rimraf docs/*",
    "release": "yarn build && yarn publish && git push --tags",
    "test": "jest",
    "test:clearCache": "./node_modules/.bin/jest --clearCache",
    "prewatch": "yarn cleanDev && yarn copy:packageJsonToDev",
    "watch": "node ./bin/watchAll.js",
    "watch:tsc": "tsc -p tsconfig.dev.json -w --module commonjs",
    "watch:test": "jest --watch --onlyChanged",
    "watch:rollup": "rollup -c rollup.config.ts -w",
    "watch:docs": "npx nodemon --exec yarn build:docsDev --watch src --ext js,mjs,json,ts,jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaggyTech/template-typescript-npm.git"
  },
  "author": "Brandon Eichler <shaggytech.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShaggyTech/template-typescript-npm/issues"
  },
  "homepage": "https://github.com/ShaggyTech/template-typescript-npm#readme",
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "jsdoc",
    "template",
    "starter",
    "prettier",
    "babel",
    "jest",
    "npm",
    "better-docs"
  ],
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "better-docs": "^1.4.7",
    "concurrently": "^5.0.2",
    "copyfiles": "^2.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsdoc": "^18.6.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-plugin-typescript": "^2.0.5",
    "jsdoc-to-markdown": "^5.0.3",
    "lodash.camelcase": "^4.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
